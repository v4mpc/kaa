{
    "manifest_version": 2,
    "name": "Kaa",
    "version": "0.1",
    "description": "An Extension that helps to Valuate Vehicles",
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://127.0.0.1/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}